{
  "name": "express-rest-typeorm",
  "version": "0.0.1",
  "description": "Rest API boilerplate with JWT authentication/authorization using TypeScript Express and TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.17.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "set debug=* && ts-node-dev ./src/index.ts --respawn --transpileOnly",
    "prod": "tsc && node ./build/app.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "start_typeorm": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  }
}
